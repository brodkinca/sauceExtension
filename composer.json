    "description": "Codeception extension to support automated testing via sauce labs.",
    "prefer-stable": true,
    "license": "GPL-3.0",
    "type": "codeception-extension",
    "keywords": ["codeception", "saucelabs", "selenium"],
    "authors": [
        {
            "name": "psychomieze",
            "email": "mail@susanne-moog.de",
            "homepage": "https://github.com/psychomieze",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "sauce/sausage": "~0.14"
    },
    "require-dev": {
        "codeception/codeception": "~1.8"
    },
    "autoload": {
        "psr-4": {
            "Codeception\\Extension\\": "extensions/sauce/"
        }
    }
